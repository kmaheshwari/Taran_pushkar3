
<div class="panel panel-default">
   
<center><h1 class="page-header text-primary"><a href="#" id="compname" data-type="text" data-original-title="Enter organization name">Organization Name</a></center></h1>
    
    </div>
<script>
    $('#compname').editable();
</script>

<div id="page-wrapper" >
    <!-- Half Page Image Background Carousel Header -->
    <header id="myCarousel" class="carousel slide">
        <!-- Indicators -->
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>

        <!-- Wrapper for Slides -->
        <div class="carousel-inner">
            <div class="item active">
                <!-- Set the first background image using inline CSS below. -->
                <div class="fill" style="background-image:url('assets/i1.jpg');"></div>
                <div class="carousel-caption">
                                    </div>
            </div>
            <div class="item">
                <!-- Set the second background image using inline CSS below. -->
                <div class="fill" style="background-image:url('assets/i3.jpg');"></div>
                <div class="carousel-caption">
                    
                </div>
            </div>
            <div class="item">
                <!-- Set the third background image using inline CSS below. -->
                <div class="fill" style="background-image:url('assets/i2.jpg');"></div>
                <div class="carousel-caption">
                    
                </div>
            </div>
        </div>

        <!-- Controls -->
        <a class="left carousel-control" href="#myCarousel" data-slide="prev">
            <span class="icon-prev"></span>
        </a>
        <a class="right carousel-control" href="#myCarousel" data-slide="next">
            <span class="icon-next"></span>
        </a>

    </header>

    <!-- Page Content -->
    <div class="container "id="page-wrapper" >

        <!-- Marketing Icons Section -->
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">
                    Welcome to Taran Pushkar
                </h1>
            </div>
            <div class="col-md-4">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h4><i class="fa fa-fw fa-check"></i> Event</h4>
                    </div>
                    <div class="panel-body">
                       
                    <% @events.each do |event| %>
                    <div class="media">
                        <div class="pull-left">
                            <span class="fa-stack fa-1x">
                                  <i class="fa fa-circle fa-stack-2x text-primary"></i>
                                  <i class="fa fa-pencil-square-o fa-stack-1x fa-inverse"></i>
                            </span> 
                        </div>
                        <div class="media-body">
                        <p><%= event.event_name %></p>
                    </div>
                </div>
                    <%end%>
                    <!-- <div class="panel-heading text-center">
                        <a href="#" class="btn btn-default">View all</a>
                    </div> -->
                    </div>
                </div>
            </div>
            <div class="col-md-4">
                <div class="panel panel-default">
                    <div class="panel-heading">
                        <h4><i class="fa fa-fw fa-check"></i> Group Events</h4>
                    </div>
                    <div class="panel-body">
                       <% @gevents.each do |event| %>
                    <div class="media">
                        <div class="pull-left">
                            <span class="fa-stack fa-1x">
                                  <i class="fa fa-circle fa-stack-2x text-primary"></i>
                                  <i class="fa fa-pencil-square-o fa-stack-1x fa-inverse"></i>
                            </span> 
                        </div>
                        <div class="media-body">
                        <p><%= event.grp_event_name %></p>
                    </div>
                </div>
                    <%end%>
                    <!-- <div class="panel-heading text-center">
                        <a href="#" class="btn btn-default">View all</a>
                    </div> -->
                    </div>
                </div>
            </div>
            
        </div>
        <!-- /.row -->

        
    </div>
    </div>
    <!-- /.container -->

    <!-- jQuery -->
    <script src="js/jquery.js"></script>

    <!-- Bootstrap Core JavaScript -->
    <script src="js/bootstrap.min.js"></script>

    <!-- Script to Activate the Carousel -->
    <script>
    $('.carousel').carousel({
        interval: 5000 //changes the speed
    })
    </script>




