<h1>Listing event_winners</h1>

<table>
  <tr>
    <th>Points</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @event_winners.each do |event_winner| %>
  <tr>
    <td><%= event_winner.points %></td>
    <td><%= link_to 'Show', event_winner %></td>
    <td><%= link_to 'Edit', edit_event_winner_path(event_winner) %></td>
    <td><%= link_to 'Destroy', event_winner, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

