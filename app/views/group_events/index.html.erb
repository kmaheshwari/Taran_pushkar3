<h1>Listing group_events</h1>

<table>
  <tr>
    <th>Grp event name</th>
    <th>Grp event type</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @group_events.each do |group_event| %>
  <tr>
    <td><%= group_event.grp_event_name %></td>
    <td><%= group_event.grp_event_type %></td>
    <td><%= link_to 'Show', group_event %></td>
    <td><%= link_to 'Edit', edit_group_event_path(group_event) %></td>
    <td><%= link_to 'Destroy', group_event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

