<h3 align="center"><strong>Race Timing for Individual Events</strong></h3>
<!--form-->
<% @mname.zip(@midlst).each do |mn,i| %>
<%= form_for(@race_timing_ind_evnt) do |f| %>
  <% if @race_timing_ind_evnt.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@race_timing_ind_evnt.errors.count, "error") %> prohibited this race_timing_ind_evnt from being saved:</h2>

      <ul>
      <% @race_timing_ind_evnt.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

   
      
      

<br><br>



                <div class="table-responsive">
                  <div class="col-xs-8 col-xs-offset-2">
                    <table class="table table-bordered table-hover table-striped" align="centre">
                        <thead>
                            <tr>
                                <th>Name</th>
                                <th>Timing</th>
                                <th>Present</th>
                            </tr>
                        </thead>
                        <tbody>
            
                            <tr>
                                <td><%= mn %></td>
                                <%= f.hidden_field :member_id, :value => i%>
