          </div>
          <!--Event Type-->
          <div class="form-group">
          
          <div class="col-sm-1 label-control">
                  <%= f.label :"Event Type" %>
          </div>
          
          <div class="col-sm-3">
            <%= f.select( :e_type, options_for_select([ "Hit","Trial"]), {} , {:class => "form-control"}) %>
          </div>
          
        </div>
