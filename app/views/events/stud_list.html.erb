<center><h1><strong>Race Timing For Individual Events</strong></h1></center>

<div id = "page-wrapper" >

    <div class="contaioner-fluid">
        <div class="form-inline row">
            <h3 class="col-md-4 col-md-offset-2"><b>Event name</b> : <%=@e.map(&:inspect).join().gsub('"','') %> </h3>
            <h3 class="col-md-6"><b>Age Group</b> : <%=@ag.map(&:inspect).join().gsub('"','') %> </h3>
        </div>
        
        <h3 style="text-align:center"><b>Total No of Students :</b> <%= @mname.length%></h3>     

          <div class="table-responsive">
                 <div class = " col-xs-6 col-xs-offset-3 ">
                    <table class="table table-bordered table-hover table-striped" align="center">
                        <thead>
                            <tr>
                                <th style="text-align:center " class="col-xs-2">S.No.</th>
                                <th style="text-align:center">Name</th>
                                
                                
                                
                            </tr>
                        </thead>

                        <tbody>
                            <!--<%= @all_names%>-->
    <br><br>
     <% i=1 %>
        <% @mname.each do |m| %> 
                            <tr>
                                <td style="text-align:center"><%= i %>

                                <td style="text-align:center"><%= m %></td>
                                <%i=i+1%>
                                 
                            </tr>
                            
        <% end %>
    
                        </tbody>
                    </table>
           
    </div>
</div>
<br>
<div class = "row">  
    <center><h4><%= link_to "Download Pdf",download_pdf_path(:format => "pdf"),:target => "_blank", :id => "pdf_link" %></h4></center>
    
        </div>
</div>