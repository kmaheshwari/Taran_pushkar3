

<!-- row -->            
<div class="row">
    <div class="col-lg-12">
        <h1 class="page-header">Listing Members Table Records</h1>
    </div>
    <!-- /.col-lg-12 -->
</div>
<!-- /.row -->

<!-- row -->
<div class="row">
    <div class="col-lg-12">
                <div class="table-responsive">
                  <table class="table table-striped table-bordered table-hover" id="dataTables-example">
                    <thead>
                      <tr>
                        <th>Competetion_name</th>
                        <th>Name</th>
                        <th>Dob</th>
                        <th>Age Group</th>
                        <th>Gender</th>
                        <th>District</th>
                        <th>State</th>
                        <th>School name or club name</th>
                        <th>Event name</th>
                        <th>Group Event name</th>

                        
                      </tr>
                    </thead>

                    <tbody>
                      
                      <% @members.zip(@list).each do |member,l| %>
                        <tr>

                          <td><%= member.comp_name %></td>
                          <td><%= member.name %></td>
                          <td><%= member.dob %></td>
                          <td><%= l[2].map(&:inspect).join()%></td>
                          <td><%= member.gender %></td>
                          <td><%= member.district %></td>
                          <td><%= member.state %></td>
                          <td><%= member.schl_club_name %></td>
                          <td><%= l[0].map(&:inspect).join(', ') %></td>
                          <td><%= l[1].map(&:inspect).join(', ') %></td>
                          <td><%= link_to 'Show', member %></td>
                          <td><%= link_to 'Edit', edit_member_path(member) %></td>
                          <td><%= link_to 'Destroy', member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
                        </tr>
                      <% end %>
                    </tbody>  
                    
              </table>
                </div>
                <!-- /.table-responsive -->    
        </div>
        <!-- /.col-lg-12 -->    
</div>
<!-- /. row -->



<%= link_to 'New Member', new_member_path %>


