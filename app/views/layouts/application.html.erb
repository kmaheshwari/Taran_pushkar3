                    </li>
                             
                    <li>
                       <i></i><%= link_to 'Add new', events_path, :class =>"fa fa-edit" %>
                    </li>                                                   
                </ul>
            </div>
            <!-- /.navbar-collapse -->
        </nav>
        
		<%= yield %>
